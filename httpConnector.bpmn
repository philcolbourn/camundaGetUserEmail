  <bpmn:process id="HttpConnectorTestProcess" name="Http Connector Test" isExecutable="true">
    <bpmn:startEvent id="ProcessStartedWithUserIdStartEvent" name="Process started with userId">
      <bpmn:outgoing>Flow_1nqfwsv</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="Flow_1nqfwsv" sourceRef="ProcessStartedWithUserIdStartEvent" targetRef="Activity_1vcgeqs" />
    <bpmn:endEvent id="EmailFoundEndEvent" name="Email found">
      <bpmn:extensionElements>
        <camunda:executionListener event="start">
          <camunda:script scriptFormat="javascript">print("Email found: " + user.email)</camunda:script>
        </camunda:executionListener>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0bjbddg</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:serviceTask id="CallRESTViaConnectorTask" name="get email for user via REST">
      <bpmn:extensionElements>
        <camunda:connector>
          <camunda:inputOutput>
            <camunda:inputParameter name="method">GET</camunda:inputParameter>
            <camunda:inputParameter name="headers">
              <camunda:map>
                <camunda:entry key="Accept">application/json</camunda:entry>
              </camunda:map>
            </camunda:inputParameter>
            <camunda:inputParameter name="url">http://localhost:8080/engine-rest/user/${userId}/profile</camunda:inputParameter>
            <camunda:outputParameter name="user">${response}</camunda:outputParameter>
          </camunda:inputOutput>
          <camunda:connectorId>http-connector</camunda:connectorId>
        </camunda:connector>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1q1lmc8</bpmn:incoming>
      <bpmn:outgoing>Flow_10qvcqj</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_10qvcqj" sourceRef="CallRESTViaConnectorTask" targetRef="Activity_1qlflte" />
    <bpmn:sequenceFlow id="Flow_0bjbddg" sourceRef="Activity_1qlflte" targetRef="EmailFoundEndEvent" />
    <bpmn:sendTask id="Activity_1qlflte" name="send email">
      <bpmn:extensionElements>
        <camunda:connector>
          <camunda:inputOutput>
            <camunda:inputParameter name="text">blah</camunda:inputParameter>
            <camunda:inputParameter name="subject">got email address!</camunda:inputParameter>
            <camunda:inputParameter name="to">${S(user).prop("email").stringValue()}</camunda:inputParameter>
          </camunda:inputOutput>
          <camunda:connectorId>mail-send</camunda:connectorId>
        </camunda:connector>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_10qvcqj</bpmn:incoming>
      <bpmn:outgoing>Flow_0bjbddg</bpmn:outgoing>
    </bpmn:sendTask>
    <bpmn:sequenceFlow id="Flow_1q1lmc8" sourceRef="Activity_1vcgeqs" targetRef="CallRESTViaConnectorTask" />
    <bpmn:userTask id="Activity_1vcgeqs" name="get user">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="userId" label="User ID" type="string" defaultValue="colbourp" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1nqfwsv</bpmn:incoming>
      <bpmn:outgoing>Flow_1q1lmc8</bpmn:outgoing>
